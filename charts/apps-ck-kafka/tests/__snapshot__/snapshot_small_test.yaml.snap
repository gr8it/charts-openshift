      name: kafka-oauth-proxy
      namespace: kafka
    spec:
      replicas: 1
      selector:
        matchLabels:
          app: kafka-oauth-proxy
      template:
        metadata:
          labels:
            app: kafka-oauth-proxy
        spec:
          containers:
            - args:
                - --http-address=:8080
                - --https-address=
                - --provider=openshift
                - --openshift-service-account=kafka-oauth-proxy
                - --upstream=http://kafka-kafka-ui.kafka.svc.cluster.local:80
                - --cookie-secret=${COOKIE_SECRET}
                - --openshift-sar={"namespace":"kafka","resource":"kafka","group":"kafka.strimzi.io","resourceName":"kafka","verb":"get"}
              env:
                - name: COOKIE_SECRET
                  valueFrom:
                    secretKeyRef:
                      key: secret
                      name: kafka-oauth-proxy-cookie-secret
              image: registry.redhat.io/openshift4/ose-oauth-proxy:v4.13
              imagePullPolicy: IfNotPresent
              name: oauth-proxy
              ports:
                - containerPort: 8080
                  name: http
              resources:
                limits:
                  cpu: 100m
                  memory: 256Mi
                requests:
                  cpu: 20m
                  memory: 128Mi
          serviceAccountName: kafka-oauth-proxy
