                key: tls.key
                secretName: kafka-brokers-tls
            name: tls
            port: 9093
            tls: true
            type: internal
        metadataVersion: 4.0-IV1
        metricsConfig:
          type: jmxPrometheusExporter
          valueFrom:
            configMapKeyRef:
              key: kafka-metrics-config.yml
              name: kafka-metrics
        rack:
          topologyKey: topology.kubernetes.io/zone
        version: 4.0.0
